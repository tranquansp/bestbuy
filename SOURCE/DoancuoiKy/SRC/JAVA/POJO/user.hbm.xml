<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="pojo.user" table="tbl_user" catalog="cuoiky">
        <id name="idu" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="nameu" type="string">
            <column name="username" length="50" not-null="true" />
        </property>
        <property name="addressu" type="string">
            <column name="address" length="150" not-null="true" />
        </property>
        <property name="phoneu" type="string">
            <column name="phone" length="20" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
