<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="pojo.category" table="tbl_category" catalog="cuoiky">
        <id name="ids" type="java.lang.Short">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="names" type="string">
            <column name="name" length="50" not-null="true" />
        </property>
        <property name="parents" type="short">
            <column name="parent" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
